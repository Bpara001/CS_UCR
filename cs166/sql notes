////////Create////////
CREATE <tablename> VALUES (___,___,);
ex: CREATE TABLE Students (SID numeric(9,0), Name text, Grade float);

INSERT INTO <tablename> VALUES ( ___,___,___);
ex:INSERT INTO Students VALUES (860507041, 'John Anderson', 3.67);


////////Querying///////
SELECT <colname> FROM <tablename> WHERE <condition>
ex: SELECT SID, Name, Grade FROM Students WHERE SID = 860507041;
ex: SELECT * FROM Students;


///////Destroy////////
DROP <tablename>
